function mkdircd () { 
    mkdir -p "$@" && eval cd "\"\$$#\""; 
}

function delall () {
    if [ ! -z "$1" ]
    then
        find . -type f -name "$1" -ls -delete
    else
        echo "please specify a file pattern"
    fi
}
function grepuncom () {
    if [ ! -z "$1" ]
    then
        grep '^[^#]' "$1"
    else
        echo "please specify a file"
    fi 
}

function swapfiles()
{
    local TMPFILE=tmp.$$
    mv "$1" $TMPFILE
    mv "$2" "$1"
    mv $TMPFILE "$2"
}

function backhome () {
    scselect Automatic
    networksetup -setairportpower en0 on
}

function gowork () {
    scselect iqiyi
    networksetup -setairportpower en0 off
}
