# This's a personal configuration. I use debian system.

# some ls aliases
alias ls='ls --color=auto'
alias ll='ls -lh'
alias la='ls -A'
alias l='ls -CF'

# some filesystem aliases
alias ..='cd ..'
alias ...='cd ../..'

# some filter aliases 
alias grep='grep --color=auto'

# some system info aliases

# some git aliases
alias gitp='git pull --rebase'
alias gitst='git status'
alias gitcl='git clone'
alias git1c='git diff HEAD HEAD~1'

# ctags
alias ctagsall='ctags -R -f .'
